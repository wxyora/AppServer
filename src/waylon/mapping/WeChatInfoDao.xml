<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="waylon.dao.WeChatInfoDao">
    
    <insert id="addWeChatInfo" parameterType="waylon.domain.WeChatUserDetailInfo" >
    	insert into wechat_user_info(openId,nickName,sex,subscribeDate,city) values (#{openId},#{nickName},#{sex},#{subscribeDate},#{city})
    </insert>
    
</mapper>